{
    "name": "lnch/laraveladmin",
    "description": "An admin panel framework for Laravel 5",
    "authors": [
        {
            "name": "Tom Lynch",
            "email": "tom@lnch.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
